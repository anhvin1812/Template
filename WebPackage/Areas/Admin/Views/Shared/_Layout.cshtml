@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    @Styles.Render("~/Admin/Content/css")
    @*@Styles.Render("~/Admin/Content/morrisChart")
    @Styles.Render("~/Admin/Content/jvectormap")
    @Styles.Render("~/Admin/Content/datePicker")
    @Styles.Render("~/Admin/Content/dateRangePicker")
    @Styles.Render("~/Admin/Content/textEditor")*@

    @RenderSection("AddStyles", required: false)
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        @Html.Partial("_Header")

        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("_SideBar")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            @RenderSection("ContentHeader", false)

            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.3.0
            </div>
            <strong>Copyright &copy; 2014-2015 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights reserved.
        </footer>
    </div><!-- ./wrapper -->


    <div id="modal-preview" class="modal fade" tabindex="-1">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content overlay-wrapper">
                    <!-- content here-->
            </div>
        </div>
    </div><!-- /#modal-preview-->

    <div id="modal" class="modal fade" tabindex="-1">
        <div class="modal-dialog" role="document">
            <div class="modal-content overlay-wrapper">
                    <!-- content here-->
            </div>
        </div>
    </div><!-- /#modal-->


@Scripts.Render("~/Admin/Js/jquery")
    <!-- jQuery UI 1.11.4 -->
    @Scripts.Render("~/Admin/Js/jqueryUI")
    @*<script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>*@
    <!-- Morris.js charts -->
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>*@
    @*@Scripts.Render("~/Admin/Js/morrisCharts")*@
    @*@Scripts.Render("~/Admin/Js/sparkline")
    @Scripts.Render("~/Admin/Js/jvectormap")
    @Scripts.Render("~/Admin/Js/jQueryKnobChart")*@
    <!-- daterangepicker -->
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.2/moment.min.js"></script>
    @Scripts.Render("~/Admin/Js/dateRangePicker")
    @Scripts.Render("~/Admin/Js/datePicker")
    @Scripts.Render("~/Admin/Js/boostrapEditor")
    @Scripts.Render("~/Admin/Js/slimScroll")*@
    @Scripts.Render("~/Admin/Js/app")

    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>

    @RenderSection("AddScripts", required: false)
</body>
</html>
